<% end -%>
<% if @append -%>
    Append   = <%= @append %>
<% end -%>
<% if @syslog -%>
    Syslog   = <%= @syslog %>
<% end -%>
<% if @catalog -%>
    Catalog  = <%= @catalog %>
<% end -%>
<% if @console -%>
    Console  = <%= @console %>
<% end -%>
<% if @mailcmd -%>
    Mail Command  = <%= @mailcmd %>
<% end -%>
<% if @mail -%>
    Mail  = <%= @mail %>
<% end -%>
<% if @operator -%>
    Operator  = <%= @operator %>
<% end -%>
<% if @operatorcmd -%>
    Operator Command = <%= @operatorcmd %>
<% end -%>
}
